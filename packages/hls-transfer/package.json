{
    "name": "@mtngtools/hls-transfer",
    "version": "0.1.0",
    "description": "HLS transfer implementations (fetch, storage)",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "vite build",
        "dev": "vite build --watch",
        "clean": "rimraf dist",
        "typecheck": "tsc --noEmit",
        "lint": "oxlint",
        "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "E2E_ENABLED=true vitest run"
    },
    "dependencies": {
        "@mtngtools/hls-types": "workspace:*",
        "ofetch": "^1.3.3"
    },
    "keywords": [
        "hls",
        "transfer",
        "http",
        "fetching"
    ],
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@types/node": "^25.1.0"
    }
}